{
  "name": "ipatool.ts",
  "version": "0.1.0",
  "description": "A tool to download IPAs from the iOS App Store",
  "main": "dist/index.js",
  "scripts": {
    "build": "wireit",
    "lint": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc --build --pretty",
      "clean": "if-file-deleted",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "lib/**",
        ".tsbuildinfo"
      ],
      "packageLocks": [
        "package-lock.json"
      ]
    },
    "lint": {
      "command": "eslint --fix src/**",
      "files": [
        "src/**/*.ts",
        ".eslintrc.yml"
      ],
      "output": []
    }
  },
  "author": "beerpsi",
  "license": "0BSD",
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/plist": "^3.0.2",
    "@types/yargs": "^17.0.10",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "typescript": "^4.6.4",
    "wireit": "^0.3.1"
  },
  "dependencies": {
    "fast-plist": "^0.1.2",
    "fetch-cookie": "^2.0.3",
    "getmac": "^5.20.0",
    "jszip": "^3.9.1",
    "node-fetch": "^3.2.4",
    "plist": "^3.0.5",
    "yargs": "^17.4.1"
  }
}
